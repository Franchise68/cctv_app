QMainWindow { background: #121212; color: #e0e0e0; }
QLabel { color: #e0e0e0; }
QToolBar { background: #1e1e1e; border: none; }
QPushButton { background: #2b2b2b; color: #e0e0e0; border: 1px solid #3a3a3a; border-radius: 6px; padding: 6px 10px; }
QPushButton:hover { background: #3a3a3a; }
QPushButton:pressed { background: #1e1e1e; }
QLineEdit { background: #1a1a1a; color: #e0e0e0; border: 1px solid #333; border-radius: 6px; padding: 6px; }
QComboBox { background: #1a1a1a; color: #e0e0e0; border: 1px solid #333; border-radius: 6px; padding: 4px; }
.statusbar { background: #1e1e1e; color: #e0e0e0; }
